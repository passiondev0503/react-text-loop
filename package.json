{
  "name": "@pr0gramm/react-text-loop-next",
  "version": "0.0.3",
  "author": "pr0gramm.com developers <contact@pr0gramm.com>",
  "license": "MIT",
  "description": "Create an animated loop of a list of text for your headings",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run build:esm && npm run build:types",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:types": "tsc --declaration --emitDeclarationOnly --declarationDir dist/types",
    "ci": "tsc --noEmit && biome ci ./src",
    "format": "biome format --write ./src",
    "lint": "biome check ./src",
    "lint:fix": "biome check --apply ./src",
    "prebuild": "rimraf dist",
    "start:examples": "parcel examples/index.html -d examples/dist --cache-dir examples/cache --open",
    "prepublishOnly": "npm run test && npm run format && npm run lint && npm run build",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/samarmohan/react-text-loop-next.git"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "react",
    "component",
    "text",
    "animation",
    "loop",
    "cycle",
    "react",
    "motion"
  ],
  "devDependencies": {
    "@biomejs/biome": "^1.4.1",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@types/jest": "^29.5.10",
    "@types/node": "^20.10.2",
    "@types/react": "^18.2.41",
    "@types/react-dom": "^18.2.17",
    "@types/react-motion": "^0.0.39",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.55.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "cxs": "^6.2.0",
    "react-motion": "^0.5.2"
  }
}
