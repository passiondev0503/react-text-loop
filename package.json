{
  "name": "@pr0gramm/react-text-loop-next",
  "version": "0.0.3",
  "author": "pr0gramm.com developers <contact@pr0gramm.com>",
  "license": "MIT",
  "description": "Create an animated loop of a list of text for your headings",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "ci": "tsc --noEmit && biome ci ./src",
    "format": "biome format --write ./src",
    "lint": "biome check ./src",
    "lint:fix": "biome check --apply ./src",
    "prebuild": "rimraf dist",
    "start:examples": "parcel examples/index.html -d examples/dist --cache-dir examples/cache --open",
    "prepublishOnly": "npm run test && npm run format && npm run lint && npm run build",
    "test": "tsc --noEmit"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/samarmohan/react-text-loop-next.git"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "react",
    "component",
    "text",
    "animation",
    "loop",
    "cycle",
    "react",
    "motion"
  ],
  "devDependencies": {
    "@biomejs/biome": "^1.4.1",
    "@testing-library/react": "^14.1.2",
    "@types/node": "^20.10.2",
    "@types/react": "^18.2.41",
    "@types/react-dom": "^18.2.17",
    "@types/react-motion": "^0.0.39",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "cxs": "^6.2.0",
    "react-motion": "^0.5.2"
  }
}
