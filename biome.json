{
    "$schema": "https://biomejs.dev/schemas/1.4.1/schema.json",
    "organizeImports": {
        "enabled": false
    },
    "formatter": {
        "enabled": true,
        "indentWidth": 2,
        "indentStyle": "space"
    },
    "vcs": {
        "enabled": true,
        "clientKind": "git"
    },
    "linter": {
        "enabled": true,
        "rules": {
            "recommended": true
        }
    },
    "files": {
        "ignore": [
            "isEqual.ts"
        ]
    }
}
